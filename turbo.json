{
  "$schema": "https://turborepo.org/schema.json",
  "tasks": {
    "lint": {
      "outputs": []
    },
    "dev": {
      "cache": false,
      "persistent": true
    },
    "build": {
      "dependsOn": ["^build"],
      "outputs": [
        "dist/**"
      ]
    },
    "build:test": {
      "dependsOn": ["^build:test"],
      "outputs": [
        "dist/**"
      ]
    }
  }
}
